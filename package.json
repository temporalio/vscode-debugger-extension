{
  "name": "debugger",
  "displayName": "temporal-debugger-plugin",
  "description": "Temporal VSCode Debugger plugin",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.68.0"
  },
  "activationEvents": [
    "onCommand:temporal-debugger-plugin.open-panel"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "temporal-debugger-plugin.open-panel",
        "title": "Temporal: Open Panel"
      }
    ]
  },
  "categories": [
    "Debuggers",
    "Visualization"
  ],
  "keywords": [
    "temporal",
    "workflow",
    "debugger",
    "vscode"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@tsconfig/node16": "^1.0.3",
    "@tsconfig/svelte": "^3.0.0",
    "@types/express": "^4.17.13",
    "@types/glob": "^7.2.0",
    "@types/humanize-duration": "^3.27.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^16.0.0",
    "@types/vscode": "^1.68.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vscode/test-electron": "^2.1.5",
    "concurrently": "^7.3.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-deprecation": "^1.3.2",
    "eslint-plugin-tsdoc": "^0.2.16",
    "humanize-duration": "^3.27.2",
    "nodemon": "^2.0.19",
    "prettier": "2.7.1",
    "rollup": "^2.77.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-svelte": "^7.1.0",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.0",
    "svelte-preprocess": "^4.10.7",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "build.extension": "tsc --build extension",
    "build.extension.watch": "tsc --build --watch extension",
    "build.webview": "rollup -c",
    "build.webview.watch": "rollup -c -w --watch.onEnd \"node scripts/reload-webview.mjs\"",
    "build.watch": "concurrently \"npm run build.extension.watch\" \"npm run build.webview.watch\"",
    "format": "prettier --write .",
    "lint": "eslint src --ext .ts --no-error-on-unmatched-pattern && prettier --end-of-line auto --check ."
  },
  "dependencies": {
    "@temporalio/client": "^1.1.0",
    "@temporalio/proto": "^1.1.0",
    "express": "^4.18.1"
  }
}
